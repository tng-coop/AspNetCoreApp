{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Blazor Server",
        "type": "coreclr",
        "request": "launch",
        "program": "${workspaceFolder}/BlazorWebApp/bin/Debug/net9.0/BlazorWebApp.dll",
        "args": [],
        "cwd": "${workspaceFolder}/BlazorWebApp",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https://aspnet.lan:\\d+)",
          "uriFormat": "%s"
        },
        "preLaunchTask": "build"
      }
    ]
  }
  
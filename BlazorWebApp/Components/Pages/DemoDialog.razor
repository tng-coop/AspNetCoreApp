@namespace BlazorWebApp.Components.Pages
@using MudBlazor

<MudDialog>
    <TitleContent>Demo Dialog</TitleContent>
    <DialogContent>
        This is the body of your demo dialog.
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Close</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    public IMudDialogInstance MudDialog { get; set; } = null!;

    void Cancel() => MudDialog.Cancel();
}

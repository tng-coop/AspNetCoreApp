{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=asp-members;Username=postgres;Password=postgres"
  },
  "JwtSettings": {
    "Issuer": "YourAppIssuer",
    "Audience": "YourAppAudience"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SmtpSettings": {
    "Server": "localhost",
    "Port": 1025,
    "FromEmail": "no-reply@example.com",
    "UseStartTls": false
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5002"
      },
      "Https": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "../cert/localhost.pfx",
          "Password": "yourpassword"
        }
      }
    }
  }
}
